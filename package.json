{
  "name": "windmillcode-tasks-zero",
  "displayName": "Windmillcode Tasks Zero",
  "description": "All tasks used at Windmillcode",
  "version": "1.88.1000",
  "publisher": "windmillcode-publisher-0",
  "icon": "images/logo.png",
  "repository": {
    "url": "https://github.com/WindMillCode/tasks-0"
  },
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension",
  "contributes": {
    "taskDefinitions": [
      {
        "type": "windmillcode",
        "properties": {
          "task": {
            "type": "string",
            "description": "the name of the windmillcode task"
          },
          "uuid": {
            "type": "string",
            "description": "the task uuid"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc ",
    "watch": "tsc  -w"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "copyfiles": "^2.4.1",
    "esbuild": "^0.19.2",
    "eslint": "^8.26.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "semver": "^7.5.4",
    "tar": "^6.1.15",
    "tar.gz2": "^1.0.0",
    "uuid": "^9.0.0"
  }
}
